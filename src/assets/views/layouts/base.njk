<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-JXVNWW10CY"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-JXVNWW10CY');
    </script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {# Page Title #}
    <title>{{ title | safe }}</title>

    {# Stylesheet #}
    <link rel="stylesheet" href="{{ '/assets/styles/styles.css' | url }}"/>

      {# Favicon #}
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/images/favicon-16x16.png">
    <link rel="manifest" href="/assets/images/site.webmanifest">
    <link rel="mask-icon" href="/assets/images/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    {# metagen plugin output #}
    {% metagen
      title = seoTitle,
      desc = seoDescription,
      url = site.siteUrl + page.url
    %}

    {# If ELEVENTY_ENV environmental variable isn't set to "production", hide this page from search engines #}
    {% if ( index == false) %}
      <meta name="robots" content="noindex,nofollow">
    {% endif %}

  </head>
  <body>
    {# Skip to content link #}
    <a class="visually-hidden" href="#main">Skip to content</a>

  

    {# Main content #}
    <main id="main" tabindex="-1">
      {% include "partials/header.njk" %}
    
      <div class="sidebar d-none d-lg-flex">
        {% include "partials/sidebar.njk" %}
      </div>
      <div class="main-wrapper">
        <div class="container">
          <div data-bs-spy="scroll" data-bs-target="#sidebar-nav" data-bs-smooth-scroll="true" tabindex="0">

            {{ content | safe }}
          </div>
        </div>
      </div>
    </main>

    

    {# Script bundle #}
    <script src="{{ '/assets/scripts/main.js' | url }}"></script>
  </body>
</html>
